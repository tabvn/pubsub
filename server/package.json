{
  "name": "pubsub-server",
  "version": "1.0.0",
  "description": "Pubsub server Node.js Websocket",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon -w src --exec \"babel-node src --presets env\"",
    "build": "babel src -s -D -d dist --presets env"
  },
  "author": "toan@tabvn.com",
  "license": "ISC",
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.0",
    "nodemon": "^1.17.2"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "lodash": "^4.17.5",
    "uuid": "^3.2.1",
    "uws": "^9.14.0"
  }
}
